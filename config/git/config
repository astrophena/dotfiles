[alias]
	code-changes = !git log --format=format: --name-only | egrep -v '^$' | sort | uniq -c | sort -rg | head -10
	pushall      = !git remote | xargs -L1 git push --all
	a            = all
	c            = commit -s
	cc           = code-changes
	l            = log
	pa           = pushall
[credential]
	helper = netrc
[core]
	autocrlf = input
[log]
	mailmap = true
[push]
	followTags = true
[pull]
	ff = only
[user]
	name       = Ilya Mateyko
	email      = astrophena@gmail.com
