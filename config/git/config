[alias]
	pushall      = !git remote | xargs -L1 git push --all
	code-changes = !git log --format=format: --name-only | egrep -v '^$' | sort | uniq -c | sort -rg | head -10
	cc           = !git code-changes
	pa           = !git pushall
	l            = log
[commit]
	gpgsign = true
[credential]
	helper = netrc
[core]
	autocrlf = input
[log]
	mailmap = true
[push]
	followTags = true
