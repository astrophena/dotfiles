DOTFILES_DIRS="$HOME/code/dotfiles"
EXCLUDES="*.md docs install.sh"
UNDOTTED="bin"

TAGS="base bash curl git kitty htop ssh vim"

if [ -f /google/devshell/bashrc.google ]; then
  TAGS="cloudshell ${TAGS}"
fi

if [ "$(uname -o)" = "Android" ]; then
  TAGS="termux ${TAGS}"
else
  TAGS="xubuntu ${TAGS}"
fi
